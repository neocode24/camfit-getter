# 텔레그램 봇 설정
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# 모니터링 설정
MONITORING_INTERVAL=5  # 분 단위
CHECK_DATE_FROM=2024-09-13
CHECK_DATE_TO=2024-09-14
ADULTS_COUNT=2
YOUTH_COUNT=2

# 대상 캠핑장
CAMP_ID=66992898908bb2001e4a650e
TARGET_ZONES=D ZONE,C ZONE

# 로그 레벨 (error, warn, info, debug)
LOG_LEVEL=info

# 브라우저 설정
HEADLESS=true                 # 창을 열지 않음 (원하면 false)
CF_MAX_WAIT_MS=15000          # CF 대기 최대 시간(밀리초)
CF_REFRESH_INTERVAL_MIN=180   # 세션 재사용 기간(분). 이 기간 내에는 브라우저 생략

# 서버 설정 (Cloudtype.io용)
PORT=3000
NODE_ENV=production

# SSL/TLS 설정 (Cloudtype.io용)
NODE_TLS_REJECT_UNAUTHORIZED=0

# Puppeteer 설정 (Cloudtype.io용)
PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true
PUPPETEER_EXECUTABLE_PATH=/usr/bin/chromium-browser
